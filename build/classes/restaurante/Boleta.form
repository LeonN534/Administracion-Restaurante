              <Properties>
                <Property name="model" type="javax.swing.table.TableModel" editor="org.netbeans.modules.form.RADConnectionPropertyEditor">
                  <Connection code="new javax.swing.table.DefaultTableModel(&#xa;    new Object [][] {&#xa;    },&#xa;    new String [] {&#xa;        &quot;Cantidad&quot;, &quot;Descripci&#xf3;n&quot;, &quot;Precio Unit.&quot;, &quot;Subtotal&quot;&#xa;    }&#xa;)&#xa;{&#xa;    @Override&#xa;    public boolean isCellEditable(int row, int column) {&#xa;        return column != 0 &amp;&amp; column != 1 &amp;&amp; column != 2 &amp;&amp; column != 3; // Puedes ajustar el &#xed;ndice de la columna que deseas hacer no editable&#xa;    }&#xa;}&#xa;" type="code"/>
                </Property>
              </Properties>
              <AuxValues>
                <AuxValue name="JavaCodeGenerator_InitCodePost" type="java.lang.String" value="String consulta2 = &quot;SELECT v.cantidad, p.Nombre AS descripcion, v.precio, v.importe &quot; +&#xa;                          &quot;FROM Ventas v &quot; +&#xa;                          &quot;JOIN Platos p ON v.idPlato = p.idPlato &quot; +&#xa;                          &quot;WHERE v.serieVenta = ?&quot;;&#xa;DefaultTableModel tableModel = (DefaultTableModel) jTable1.getModel();&#xa;try (&#xa;        // Establecer la conexi&#xf3;n a la base de datos&#xa;        Connection connection = DriverManager.getConnection(URL, USERNAME, PASSWORD);&#xa;&#xa;        // Preparar la consulta SQL con un PreparedStatement&#xa;        PreparedStatement preparedStatement = connection.prepareStatement(consulta2);&#xa;) {&#xa;    // Establecer el valor del par&#xe1;metro serieVenta en la consulta&#xa;    preparedStatement.setString(1, String.valueOf(this.serieVenta));&#xa;&#xa;    // Ejecutar la consulta&#xa;    ResultSet resultSet = preparedStatement.executeQuery();&#xa;&#xa;    // Limpiar el modelo actual de la tabla&#xa;    tableModel.setRowCount(0);&#xa;&#xa;    // Llenar el modelo con los resultados de la consulta&#xa;    while (resultSet.next()) {&#xa;        Object[] rowData = {&#xa;                resultSet.getInt(&quot;cantidad&quot;),&#xa;                resultSet.getString(&quot;descripcion&quot;),&#xa;                resultSet.getDouble(&quot;precio&quot;),&#xa;                String.valueOf(resultSet.getInt(&quot;cantidad&quot;)*resultSet.getDouble(&quot;precio&quot;))&#xa;        };&#xa;        tableModel.addRow(rowData);&#xa;        importe = resultSet.getFloat(&quot;importe&quot;);&#xa;    }&#xa;&#xa;&#xa;&#xa;} catch (SQLException e) {&#xa;    e.printStackTrace();&#xa;}&#xa;for (int i = 0; i &lt; jTable1.getRowCount(); i++) {&#xa;    total += Float.parseFloat(String.valueOf(jTable1.getValueAt(i, 3)));&#xa;}&#xa;&#xa;vuelto = importe - total;"/>
              </AuxValues>
            </Component>
